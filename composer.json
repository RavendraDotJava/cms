{
    "require": {
        "bymayo/porter": "1.0.3",
        "craftcms/cms": "4.5.5",
        "craftcms/commerce": "4.2.11",
        "craftcms/webhooks": "3.0.4",
        "digital-pros/commerce-authorize": "4.0.3",
        "doublesecretagency/craft-cpcss": "2.6.0",
        "doublesecretagency/craft-inventory": "2.2.0",
        "nystudio107/craft-minify": "4.0.0-beta.2",
        "nystudio107/craft-seomatic": "4.0.28",
        "nystudio107/craft-typogrify": "4.0.1",
        "percipioglobal/craft-colour-swatches": "4.3.0",
        "percipioglobal/craft-password-policy": "4.1.0",
        "poptin/poptin": "2.0.3",
        "sebastianlenz/linkfield": "2.1.5",
        "servd/craft-asset-storage": "3.4.7",
        "solspace/craft-freeform": "4.1.0",
        "surprisehighway/craft-avatax": "3.0.1",
        "vaersaagod/matrixmate": "2.1.3",
        "verbb/navigation": "2.0.18",
        "verbb/super-table": "3.0.9",
        "verbb/vizy": "2.1.6",
        "vlucas/phpdotenv": "^5.4.0",
        "wmd/section-and-product-type": "2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\ccfieldsmodule\\": "modules/ccfieldsmodule/src/",
            "modules\\cchelpersmodule\\": "modules/cchelpersmodule/src/",
            "modules\\ccapimodule\\": "modules/ccapimodule/src/",
            "modules\\ccdocsmodule\\": "modules/ccdocsmodule/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true,
            "php-http/discovery": true
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.craftcms.com",
            "canonical": false
        }
    ]
}